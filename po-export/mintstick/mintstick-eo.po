msgstr "Subskribita de: %s"

#: lib/verify.py:287
msgid "Unknown signature"
msgstr "Nekonata subskribo"

#: lib/verify.py:288
msgid "Key not found on keyserver."
msgstr "Ŝlosilo ne trovita sur ŝlosilservilo."

#: lib/verify.py:299 lib/verify.py:305
msgid "Everything looks good!"
msgstr "Ĉio aspektas bone!"

#: lib/verify.py:300
msgid "This is an official ISO image."
msgstr "Ĉi tio estas oficiala ekzakta kopio."

#: lib/verify.py:306
msgid "This ISO image is verified by a trusted signature."
msgstr "Ĉi tiu ekzakta kopio estas estas konfirmita de fidinda subskribo."

#: lib/verify.py:309
msgid "If you trust the signature you can trust the ISO."
msgstr "Se vi fidas la subskribon, vi povas fidi la ekzaktan kopion."

#: lib/verify.py:310
msgid "Untrusted signature"
msgstr "Nefidinda subskribo"

#: lib/verify.py:311
msgid "This ISO image is verified by an untrusted signature."
msgstr "Ĉi tiu ekzakta kopio estas konfirmita de nefidinda subskribo."

#: lib/verify.py:316
msgid "An error occurred"
msgstr "Okazis eraro"

#: lib/verify.py:327
msgid "The SHA256 sum of the ISO image is incorrect."
msgstr "La sha256-sumo de la ekzakta kopio estas neĝusta."

#: lib/verify.py:328
msgid "The SHA256 sums file does not contain sums for this ISO image."
msgstr ""
"La dosiero kun la sha256-sumoj ne enhavas sumojn por ĉi tiu ekzakta kopio."

