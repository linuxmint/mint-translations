msgstr "Потписао је: %s"

#: lib/verify.py:287
msgid "Unknown signature"
msgstr "Непознат потпис"

#: lib/verify.py:288
msgid "Key not found on keyserver."
msgstr "Нисам нашао кључ на серверу."

#: lib/verify.py:299 lib/verify.py:305
msgid "Everything looks good!"
msgstr "Све изгледа добро!"

#: lib/verify.py:300
msgid "This is an official ISO image."
msgstr "Ово је званичан ISO одраз."

#: lib/verify.py:306
msgid "This ISO image is verified by a trusted signature."
msgstr "Овај ISO одраз је проверен поверљивим потписом."

#: lib/verify.py:309
msgid "If you trust the signature you can trust the ISO."
msgstr "Ако верујете потпису можете веровати и у ISO."

#: lib/verify.py:310
msgid "Untrusted signature"
msgstr "Неповерљив потпис"

#: lib/verify.py:311
msgid "This ISO image is verified by an untrusted signature."
msgstr "Овај ISO одраз је проверио неповерљив потпис."

#: lib/verify.py:316
msgid "An error occurred"
msgstr "Дошло је до грешке"

#: lib/verify.py:327
msgid "The SHA256 sum of the ISO image is incorrect."
msgstr "SHA256 сума ISO одраза није тачна."

#: lib/verify.py:328
msgid "The SHA256 sums file does not contain sums for this ISO image."
msgstr "Датотека SHA256 сума не садржи суме за овај ISO одраз."

