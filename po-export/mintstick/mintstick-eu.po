msgstr "ISO irudi hau sinadura fidagarri baten bidez egiaztatu da."

#: lib/verify.py:309
msgid "If you trust the signature you can trust the ISO."
msgstr "Sinaduraz fidatzen bazara, ISOaz fida zaitezke."

#: lib/verify.py:310
msgid "Untrusted signature"
msgstr "Sinadura ez fidagarria"

#: lib/verify.py:311
msgid "This ISO image is verified by an untrusted signature."
msgstr "ISO irudi hau fidagarria ez den sinadura baten bidez egiaztatu da."

#: lib/verify.py:316
msgid "An error occurred"
msgstr "Errore bat gertatu da"

#: lib/verify.py:327
msgid "The SHA256 sum of the ISO image is incorrect."
msgstr "ISO irudiaren SHA256 batura okerra da."

#: lib/verify.py:328
msgid "The SHA256 sums file does not contain sums for this ISO image."
msgstr "SHA256 baturen fitxategiak ez du ISO irudi honen baturarik."

