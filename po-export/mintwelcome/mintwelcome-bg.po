msgid "Welcome"
msgstr "Добре дошли"

#: usr/lib/linuxmint/mintwelcome/mintwelcome.py:113
msgid "First Steps"
msgstr "Първи стъпки"

#: usr/lib/linuxmint/mintwelcome/mintwelcome.py:117
